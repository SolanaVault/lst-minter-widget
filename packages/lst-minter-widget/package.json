{
  "name": "@the-vault/lst-minter-widget",
  "version": "0.0.1",
  "description": "The Vault LST minter react widget",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:svg": "svgr --typescript --icon --out-dir src/icons src/icons",
    "build": "npm run build:svg && tsc && cp src/styles.css dist/styles.css",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@tanstack/react-query": "^5.65.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "typescript": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SolanaVault/lst-minter-widget"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "The Vault",
  "license": "MIT"
}
